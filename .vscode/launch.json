    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Tests",
            "type": "python",
            "request": "launch",
            "purpose": [
                "debug-test"
            ],
            "program": "${file}",
            "justMyCode": false,
            "console": "integratedTerminal",
        },
        {
            "name": "Debug Ingest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}//runner.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "ingest",
                "storage/log.zip",
            ]
        },
        {
            "name": "Debug VAP",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}//runner.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "vap",
                "pipelines/vap_wave_raw/config/pipeline.yaml",
                "--begin",
                "20210828.000000",
                "--end",
                "20210829.000000"
            ]
        },
        {
            "name": "Debug VAP2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}//runner.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "vap",
                "pipelines/vap_wave_stats/config/pipeline.yaml",
                "--begin",
                "20210901.000000",
                "--end",
                "20211001.000000"
            ]
        },
    ]
}
